{
  "name": "Gmail Sender Icons by Simbolic",
  "description": "Quickly identify the sender of email messages in Gmail without opening the message.",
  "version": "1.0",
  "content_scripts": [{
    "matches": [
      "https://mail.google.com/*"
    ],
    "js": ["inboxsdk.js", "content.js"],
    "css": ["styles.css"],
    "run_at": "document_end"
  }],
  "-content_security_policy": {
    "extension_pages": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://mail.google.com/"
  ],
  "manifest_version": 3,
  "short_name": "Gmail Sender Icons",
  "author": "Simbolic",
  "homepage_url": "https://www.simbolic.net/",
  "-web_accessible_resources": [
    "*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Show sender icons in Gmail",
    "default_popup": "options.html"
  },
  "icons": {
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png",
    "256": "icon.png"
  }
}